{
  "manifest_version": 2,

  "name": "Fantasy Hype Train",
  "description": "Shows players who did well!",
  "version": "1.0",
  "background": {
     "scripts": [
       "./lib/backgroundEntry.js"
     ],
     "persistent": true
   },
  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
 },
 "permissions": [
      "storage"
    ]
}
